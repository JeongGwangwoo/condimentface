    <script>
        function readURL(input) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();
                reader.onload = function(e) {
                    $('.image-upload-wrap').hide();
                    $('#loading').show();
                    $('.file-upload-image').attr('src', e.target.result);
                    $('.file-upload-content').show();
                    $('.image-title').html(input.files[0].name);
                };
                reader.readAsDataURL(input.files[0]);
                init().then(function() {
                    console.log("hello");
                    predict();
                    $('#loading').hide();
                });
            } else {
                removeUpload();
            }
        }

        function removeUpload() {
            $('.file-upload-input').replaceWith($('.file-upload-input').clone());
            $('.file-upload-content').hide();
            $('.image-upload-wrap').show();
        }
        $('.image-upload-wrap').bind('dragover', function() {
            $('.image-upload-wrap').addClass('image-dropping');
        });
        $('.image-upload-wrap').bind('dragleave', function() {
            $('.image-upload-wrap').removeClass('image-dropping');
        });
    </script>
    <script src="https://cdn.jsdelivr.net/npm/@tensorflow/tfjs@1.3.1/dist/tf.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@teachablemachine/image@0.8/dist/teachablemachine-image.min.js"></script>
    <script type="text/javascript">
        let URL;
        const urlMale = "https://teachablemachine.withgoogle.com/models/l1J_qBrFV/";
        const urlFemale = "https://teachablemachine.withgoogle.com/models/l1J_qBrFV/";
        let model, webcam, labelContainer, maxPredictions;
        async function init() {
            if (document.getElementById("gender").checked) {
                URL = urlMale;
            } else {
                URL = urlFemale;
            }
            const modelURL = URL + "model.json";
            const metadataURL = URL + "metadata.json";
            model = await tmImage.load(modelURL, metadataURL);
            maxPredictions = model.getTotalClasses();
            labelContainer = document.getElementById("label-container");
            for (let i = 0; i < maxPredictions; i++) {
                var element = document.createElement("div")
                element.classList.add("d-flex");
                labelContainer.appendChild(element);
            }
        }
        async function predict() {
            var image = document.getElementById("face-image")
            const prediction = await model.predict(image, false);
            prediction.sort((a, b) => parseFloat(b.probability) - parseFloat(a.probability));
            console.log(prediction[0].className);
            var resultTitle, resultExplain, resultCeleb;
            if (document.getElementById("gender").checked) {
                switch (prediction[0].className) {
                    case "shoyu":
                        resultTitle = "간장 얼굴상"
                        resultExplain = " 소유가오를 정의하자면 ‘가장 일본인스러운 얼굴’이라고 할 수 있습니다. 쇼유가오의 특징을 좀 더 자세히 말하자면 눈꼬리가 째진 듯한 눈, 예쁘게 서있는 콧날, 전체적으로 작은 얼굴에 고운 얼굴 선으로 표현할 수 있는데요. 1980년대 이후의 일본 아이돌 중 많은 이들이 소유가오를 가지고 있다고 합니다. 게다가 쇼유가오는 뒤에서 소개해드릴 소스가오(ソース顔)와 함께 지금의 ～顔(가오) 시리즈의 시초라고도 말할 수 있습니다. 대략 1987년부터 여자들이 남자들의 얼굴을 구분하며 이야기 할 때 이목구비가 진하지 않고 담백하게 생긴 얼굴을 ‘소유가오’, 정반대로 이목구비가 진하고 우리나라 표현으로 조금은 느끼하게 생긴 얼굴을 ‘소스가오’라고 부르기 시작하며 그것이 유행을 타게 되어 지금의 여러 ～顔(가오) 시리즈가 생겨나게 됐다는 것이죠. 각각 간장이 담백하고, 소스가 진한 맛을 가진 조미료라는 점을 잘 반영한 표현입니다. "
                        resultCeleb = "간장 얼굴상 연예인: 기무라 타쿠야, 니노미야 카즈나리, 오구리 슌, 사토 타케루..."
                        break;
                    case "miso":
                        resultTitle = "된장 얼굴상"
                        resultExplain = " 미소가오는 한 마디로 ‘소스가오 처럼 진한 이목구비를 가지고 있지만, 소스가오보다 조금 더 부드러운 얼굴’ 이라고 말할 수 있습니다. 소스가오와 확실히 구분 짓기는 힘들지만, 굳이 구분을 하자면 미소가오가 소스가오보다 조금 덜 서양적인 얼굴이라고 말할 수 있겠네요. 그런데 일본인 친구들에게 물어보니 미소가오를 모르는 사람들도 있을 만큼 다른 표현들에 비해 남자의 얼굴을 구분하는 기준 중 하나이긴 하지만 널리 쓰이고 있지 않는 표현이라고 합니다. "
                        resultCeleb = "된장 얼굴상 연예인: 마츠다이라 켄, 사카구치 켄지, 오다기리 죠, 이케우치 히로유키..."
                        break;
                    case "mayo":
                        resultTitle = "마요네즈 얼굴상"
                        resultExplain = " 마요네즈가오는 ‘사토가오 정도로 엄청 귀여운 얼굴은 아닌, 좀 더 깔끔한 얼굴’ 이라고 할 수 있는 얼굴입니다. "
                        resultCeleb = "마요네즈 얼굴상 연예인: 타카하시 요시노부, 시부타니 스바루, 오카다 마사키, 후지와라 타츠야..."
                        break;
                    case "satou":
                        resultTitle = "설탕 얼굴"
                        resultExplain = " 사토우가오는 한마디로 간단히 얘기하자면 ‘몇 살이 되어도 소년다움이 남아있는 베이비 페이스’라고 말할 수 있습니다. 사토우가오의 특징으로는 여자보다 귀여운 똥글똥글한 눈, 예쁜 웃는 얼굴 등이 있으며, 다른 표현들과 마찬가지로 사토우가오도 ‘사랑스러움→달콤하다→ 설탕’ 이라는 일종의 연상법으로 생긴 표현이라고 합니다. 사토우가오의 남자들은 여자라고 해도 믿을 만큼의 예쁜 얼굴을 가지고 있는데요. "
                        resultCeleb = "설탕 얼굴 연예인: 나카지마 켄토, 니시지마 타카히로, 코이케 텟페이, 치바 유다이..."
                        break;
                    case "shio":
                        resultTitle = "소금 얼굴상"
                        resultExplain = " 시오가오로 불리는 남자들의 특징은 얼굴로만 봤을 때는 한 마디로 ‘이목구비가 진하지 않은 사람’이라고 할 수 있습니다. 이것을 일본어로 말하자면 ‘顔が薄い(かおがうすい)-카오가우스이’라고 말하는데요. 한국어로 직역하자면 ‘얼굴이 연하다’ 라고 해석할 수 있습니다. 시오가오의 특징을 좀 더 자세히 말하자면 쌍꺼풀이 없거나 속 쌍꺼풀의 샤프한 눈매, 하얀 피부, 큰 키, 청량감이 있는 자연스러운 헤어스타일, 전체적으로 깔끔하게 생겨 계속 봐도 질릴 것 같지 않은 용모, 아름다운 얼굴형부터 목까지의 실루엣, 그리고 조금은 여리여리한 겉모습과는 달리 목젖이나 쇄골에서 의외로 느껴지는 남자다움과 섹시함 등이 있습니다. 좀 더 재미있게 표현된 특징으로는 검정 테 안경, 핑크색, 낡은 티셔츠가 어울릴 것 같은 사람 등이 있죠. 이렇게 시오가오가 다양하고 자세한 특징들로 표현되고 있는 이유는 현재 일본에서 가장 인기가 많은 남자 얼굴이 바로 이 시오가오이기 때문이라고 합니다. 한국인들이 흔히 말하는 훈남 스타일도 바로 시오가오에 가장 가깝죠. 그렇기 때문에 훈남 스타일을 좋아하시는 분들이라면 대부분 시오가오를 가장 선호하실 것 같습니다. "
                        resultCeleb = "소금 얼굴상 연예인: 나카무라 토모야, 다나카 케이, 후쿠시 소우타, 카미키 류노스케..."
                        break;
                    case "sause":
                        resultTitle = "소스 얼굴상"
                        resultExplain = " 소스가오는 ‘이목구비가 진하고 서양적인 얼굴’ 입니다. 보통 돈카츠 소스, 굴 소스, 불고기 소스 등 소스의 종류 대부분이 조금 걸죽하고 진한 맛을 가진 것처럼 소스가오도 그런 느낌이 나는 얼굴이라고 생각하시면 됩니다. 소유가오 혹은 시오가오와 정반대의 얼굴이 바로 소스가오라고 생각하시면 가장 이해가 빠를 것 같네요. 훈남 스타일을 선호하는 저로서는 조금은 느끼하다고 생각하는 얼굴이기도 한데요. "
                        resultCeleb = "소스 얼굴상: 나가세 토모야, 딘 후지오카, 마츠모토 준, 아베 히로시, 야마다 타카유키, 아라타 마켄유..."
                        break;
                    case "su":
                        resultTitle = "식초 얼굴상"
                        resultExplain = "소금 얼굴보다 더 담백하게 생긴 얼굴"
                        resultCeleb = "식초 얼굴상 연예인: 모리야마 미라이, 이노라하 요시히코..."
                        break;
                    case "olive":
                        resultTitle = "올리브유 얼굴상"
